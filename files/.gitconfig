[alias]
  diffs = diff --staged
  st = status
[color]
  branch = auto
  diff = auto
  grep = auto
  interactive = auto
  status = auto
[core]
  autocrlf = input
  editor = vim
  excludesfile = ~/.gitignore.global
[include]
  path = ~/.gitconfig.local
[pager]
  branch = false
[rebase]
  autosquash = true
[user]
  email = utagawakiki@gmail.com
  name = UTAGAWA Kiki
