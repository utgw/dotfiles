[user]
  email = utagawakiki@gmail.com
  name = UTAGAWA Kiki
[color]
  branch = auto
  diff = auto
  grep = auto
  interactive = auto
  status = auto
[core]
  autocrlf = input
  editor = vim
  excludesfile = ~/.gitignore.global
[alias]
  bl = blame
  br = branch
  cm = commit
  co = !echo 'co (checkout) 使うのをやめてswitchやrestoreを使うようにしましょう'
  diffs = diff --staged
  dump = cat-file -p
  hist = log --pretty=format:\"%h %ad | %s%d [%an]\" --graph --date=short
  ln = log --oneline
  pfm = push --force origin master
  pm = push origin master
  st = status
  sta = stash
  type = cat-file -t
[rebase]
  autosquash = true
[include]
  path = ~/.gitconfig.local
