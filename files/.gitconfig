[alias]
  br = branch
  co = checkout
  diffs = diff --staged
  hist = log --pretty=format:\"%h %ad | %s%d [%an]\" --graph --date=short
  ln = log --oneline
  st = status
  sw = switch
[color]
  branch = auto
  diff = auto
  grep = auto
  interactive = auto
  status = auto
[core]
  autocrlf = input
  editor = vim
  excludesfile = ~/.gitignore.global
[include]
  path = ~/.gitconfig.local
[pager]
  branch = false
[rebase]
  autosquash = true
[user]
  email = utagawakiki@gmail.com
  name = UTAGAWA Kiki
